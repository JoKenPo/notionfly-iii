{
  "root": true,
  "extends": "@react-native-community",
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "overrides": [
    {
      "files": [
        "*.ts",
        "*.tsx",
        "*.js"
      ],
      "rules": {
        "prettier/prettier": 0,
        "@typescript-eslint/no-shadow": [
          "error"
        ],
        "no-shadow": "off",
        "no-undef": "off"
      }
    }
  ]
}